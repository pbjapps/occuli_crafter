<div class="container">

  {{#each oculi in model}}
    {{selectable-oculi oculi=oculi action="selectOculi"}}
  {{/each}}

  <h3 class="text-center"> Crafter </h3>
  {{selectable-oculi oculi=crafter.slot1 action="deselectOculi" slot="slot1"}}
  {{selectable-oculi oculi=crafter.slot2 action="deselectOculi" slot="slot2"}}
  {{selectable-oculi oculi=crafter.slot3 action="deselectOculi" slot="slot3"}}

  <h3 class="text-center"> Result </h3>
  {{selectable-oculi oculi=crafter.result action="craftOculi" crafter=crafter}}


  <h3 class="text-center"> Selected Oculi </h3>
  <div class="text-center crafter-list">
  </div>


</div>
